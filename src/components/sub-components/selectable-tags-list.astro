---
import ListboxItem from "./listbox-item.astro";

interface Props {
    tagsType: string,
    listboxId: string,
    tagsList: string[],
}
const { tagsType, listboxId, tagsList } = Astro.props;
---
<ul class="selectable-tags-list" data-tags-type={tagsType} id={listboxId} data-height-null="true" hidden>
    {tagsList.map((tag) => <ListboxItem {tag} />)}
</ul>